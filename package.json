{
  "name": "react-native-calendario",
  "version": "0.5.0",
  "description": "React Native Calendar",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "test": "npm run lint",
    "flow": "flow",
    "prepublish": "npm run lint"
  },
  "lint-staged": {
    "**/*.js": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,jsx}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maggialejandro/react-native-calendario.git"
  },
  "keywords": [
    "react-native",
    "calendar"
  ],
  "author": "Alejandro Maggi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maggialejandro/react-native-calendario/issues"
  },
  "homepage": "https://github.com/maggialejandro/react-native-calendario#readme",
  "dependencies": {
    "moment": "2.24.0"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/react": "16.8.14",
    "@types/react-native": "0.57.46",
    "babel-eslint": "10.0.3",
    "eslint": "6.6.0",
    "eslint-config-satya164": "3.1.2",
    "flow-bin": "0.92.0",
    "husky": "3.0.9",
    "lint-staged": "9.5.0",
    "metro-react-native-babel-preset": "0.54.0",
    "prettier": "1.19.1",
    "react": "16.8.3",
    "react-native": "0.59.8"
  }
}
